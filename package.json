{
  "name": "bigv-portfolio-css",
  "scripts": {
    "build:dev": "npx tailwindcss -i src/css/input.css -o resources/public/css/style.css --watch",
    "build:prod": "npx tailwindcss -c tailwind.config.js -i src/css/input.css -o dist/css/style.css --minify",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "papaparse": "^5.3.2"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.11",
    "daisyui": "^5.0.46",
    "gh-pages": "^6.3.0",
    "tailwindcss": "^4.1.11"
  }
}
